<div class='form-group'>
    <%= select("order", "customer_id", Customer.all.collect {|p| [p.name, p.id] }, { include_blank: true}, :class => 'form-control')%>
</div>
<div class='form-group'>
<%= c.label :name, 'Name' %>
<%= c.text_field :name, :class => "form-control" %>
</div>
<div class='form-group'>
<%= c.label :address, 'Address' %>
<%= c.text_field :address, :class => "form-control" %>
</div>
<div class='form-group'>
<%= c.label :mobile, 'Mobile' %>
<%= c.text_field :mobile, :class => "form-control" %>
</div>
<div class='form-group'>
<%= c.label :cover_photo %>
<%= c.file_field :cover_photo %>
</div>

<%= c.submit "submit", :class => "btn btn-primary" %>
